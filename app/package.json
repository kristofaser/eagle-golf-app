{
  "name": "eagle",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:unit": "jest --testPathPattern=__tests__/unit",
    "test:integration": "jest --testPathPattern=__tests__/integration",
    "test:services": "jest --testPathPattern=__tests__/services",
    "test:hooks": "jest --testPathPattern=__tests__/hooks",
    "test:components": "jest --testPathPattern=__tests__/components",
    "test:changed": "jest -o",
    "test:critical": "scripts/test-critical.sh",
    "test:update-snapshots": "jest -u",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "quality": "npm run lint && npm run type-check && npm test",
    "migrate:postgis": "node scripts/migrate-golf-parcours-postgis.js",
    "test:postgis": "node scripts/test-postgis-migration.js",
    "test:postgis-full": "node scripts/test-postgis-migration.js --create-tests",
    "prepare": "husky"
  },
